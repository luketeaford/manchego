{
  "name": "manchego",
  "version": "1.0.0",
  "description": "Cheesy CLI",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf coverage .nyc_output",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard --fix",
    "prepublishOnly": "npm run lint && npm test",
    "test": "nyc --reporter=lcov tape test/*.test.js | tap-spec"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luketeaford/manchego.git"
  },
  "keywords": [
    "manchego",
    "cheesy",
    "cli",
    "tool"
  ],
  "author": "Luke Teaford",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/luketeaford/manchego/issues"
  },
  "homepage": "https://github.com/luketeaford/manchego#readme",
  "devDependencies": {
    "coveralls": "^3.0.3",
    "nyc": "^13.3.0",
    "standard": "^12.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1"
  }
}
